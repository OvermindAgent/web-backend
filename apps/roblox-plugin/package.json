    "watch": "rbxtsc -w --type=model -i include"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "latest",
    "@rbxts/types": "latest",
    "roblox-ts": "latest",
    "typescript": "latest"
  },
  "dependencies": {
    "@rbxts/roact": "latest",
    "@rbxts/roact-hooked": "latest",
    "@rbxts/services": "latest"
  }
}
